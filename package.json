{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "expo": "^33.0.0",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.0",
    "react-test-renderer": "^16.8.6",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.1.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "jest": "24.1.0",
    "jest-expo": "^33.0.2"
  },
  "jest": {
    "preset": "jest-expo",
    "coveragePathIgnorePatterns": [
      "/app/test/",
      "/app/constants/",
      "index.js",
      "/app/AppNavigator.js",
      "/app/config/Routes.js",
      "/app/store.js"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/app/test/setup.js"
    ]
  },
  "private": true
}
